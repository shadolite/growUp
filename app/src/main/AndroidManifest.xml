<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cs246.growup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Views.CalendarActivity" />
        <activity
            android:name=".Views.AddGoalView"
            android:exported="true"
            android:label="@string/add_goal_view_label"
            android:windowSoftInputMode="stateVisible" />
        <activity
            android:name=".Views.AddEntryView"
            android:exported="true"
            android:label="@string/add_entry_view_label"/>
        <activity android:name=".Views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Views.SearchResultsView"
            android:exported="true"></activity>
    </application>

</manifest>